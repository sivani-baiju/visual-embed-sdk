{{#if isInPath}}
</ul>
<ul class="current">
    {{/if}}
    <li class="{{#if isInPath}}current{{/if}} {{cssClasses}}">
        {{#if groupTitle}}
        <div class="group-title">{{{wbr groupTitle}}}</div>
        {{else}}
        <a href="{{relativeURL url}}" class="tsd-kind-icon">{{{wbr title}}}</a>
        {{/if}}
        {{#if children}}
        <ul>
            {{#each children}}
            {{> toc}}
            {{/each}}
        </ul>
        {{/if}}
    </li>
    {{#if isInPath}}
</ul>
<ul class="after-current">
    {{/if}}